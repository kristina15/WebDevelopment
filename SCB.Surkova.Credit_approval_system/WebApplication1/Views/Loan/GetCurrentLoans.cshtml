@using WebApplication1.Models.LoanVMs;
@model IEnumerable<DisplayLoanVM>

@{
    ViewBag.Title = "GetCurrectLoans";
}

@section scripts{
    <script src="~/Scripts/ApiScripts/MyScripts.js"></script>
}
<br />
@if (Model.Count() == 0)
{
    @:(not found)
}
else
{
    <table class="table" id="loanTable">
        <tbody>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Sum)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DateCreate)
                </th>
                <th></th>
                <th></th>
            </tr>
            @Html.Partial("~/Views/Shared/LoanPartial/_UndewriterLoansPartial.cshtml", Model)
        </tbody>
    </table>
}
